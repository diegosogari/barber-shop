type Service {
    id: Int!
    createdAt: Int!
    updatedAt: Int!
    deletedAt: Int
    name: String!
    cost: Int!
    notes: String
}

input ServiceInput {
    name: String
    cost: Int
    notes: String
}

type ServiceQuery {
  listService: [Service!]!
  getService(id: Int!): Service!
}

type ServiceMutation {
  createService(input: ServiceInput!): Service!
  updateService(id: Int!, input: ServiceInput!): Service!
  deleteService(id: Int!): Service!
}
